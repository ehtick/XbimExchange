<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net47;netstandard2.0</TargetFrameworks>
    <Title>Xbim.CobieLiteUK</Title>
    <Description>UK COBieLite schema for the XBIM Exchange toolkit</Description>
    <AssemblyName>Xbim.CobieLiteUK</AssemblyName>
    <RootNamespace>Xbim.CobieLiteUk</RootNamespace>
    <PackageId>Xbim.COBieLiteUK</PackageId>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="FilterHelper\COBieArchitecturalFilters.config" />
    <None Remove="FilterHelper\COBieDefaultFilters.config" />
    <None Remove="FilterHelper\COBieElectricalFilters.config" />
    <None Remove="FilterHelper\COBieFireProtectionFilters.config" />
    <None Remove="FilterHelper\COBieMechanicalFilters.config" />
    <None Remove="FilterHelper\COBiePlumbingFilters.config" />
    <None Remove="Templates\UK2012.xls" />
    <None Remove="Templates\UK2012.xlsx" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="FilterHelper\COBieArchitecturalFilters.config" />
    <EmbeddedResource Include="FilterHelper\COBieDefaultFilters.config" />
    <EmbeddedResource Include="FilterHelper\COBieElectricalFilters.config" />
    <EmbeddedResource Include="FilterHelper\COBieFireProtectionFilters.config" />
    <EmbeddedResource Include="FilterHelper\COBieMechanicalFilters.config" />
    <EmbeddedResource Include="FilterHelper\COBiePlumbingFilters.config" />
    <EmbeddedResource Include="Templates\UK2012.xls" />
    <EmbeddedResource Include="Templates\UK2012.xlsx" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="Newtonsoft.Json.Bson" Version="1.0.1" />
    <PackageReference Include="NPOI" Version="2.4.0" />
    <PackageReference Include="Xbim.Essentials" Version="5.1.259" />
  </ItemGroup>

</Project>
